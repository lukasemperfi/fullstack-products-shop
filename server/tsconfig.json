{
    "compilerOptions": {
        "target": "ES2015",
        "module": "commonjs",
        "lib": [
            "es6"
        ],
        "allowJs": true,
        "outDir": "./build",
        "strict": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "strictPropertyInitialization": false,
    },
}